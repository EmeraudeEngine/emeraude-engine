prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@CMAKE_INSTALL_PREFIX@

lib=${exec_prefix}/@CMAKE_INSTALL_LIBDIR@
include=${prefix}/@CMAKE_INSTALL_INCLUDEDIR@

Name: @PROJECT_NAME@
Description: @PROJECT_DESCRIPTION@
Version: @PROJECT_VERSION@

Requires:
Libs: -L${exec_prefix}/@CMAKE_INSTALL_LIBDIR@ -l@PROJECT_NAME@
Cflags: -I${include}/EmeraudeLibraries -I${include}/Emeraude
